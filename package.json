    "husky": "7.0.2",
    "jsdom": "^19.0.0",
    "lint-staged": "11.1.2",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.6",
    "postcss-html": "^1.3.0",
    "postcss-import": "14.0.0",
    "postcss-scss": "^4.0.3",
