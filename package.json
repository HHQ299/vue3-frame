{
  "name": "student-service-center",
  "version": "1.3.1",
  "private": true,
  "engines": {
    "node": ">= 16",
    "pnpm": ">= 6"
  },
  "scripts": {
    "dev": "cross-env --max_old_space_size=4096 vite",
    "dev:test": "cross-env --max_old_space_size=4096 vite --mode developmenttest",
    "dev:mock": "cross-env --max_old_space_size=4096 vite --mode mock",
    "serve": "pnpm dev",
    "build": "rimraf dist && cross-env vite build",
    "build:dev": "rimraf dist && cross-env vite build --mode dev",
    "build:test": "rimraf dist && cross-env vite build --mode test",
    "build:prod": "rimraf dist && cross-env vite build",
    "report": "rimraf dist && cross-env vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "preview": "vite preview",
    "preview:build": "pnpm build && vite preview",
    "clean:cache": "rm -rf node_modules && rm -rf .eslintcache && pnpm install",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,css,scss,postcss,less}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.0",
    "@element-plus/icons": "^0.0.11",
    "@element-plus/icons-vue": "^1.0.1",
    "@types/ali-oss": "^6.16.3",
    "@types/qrcode": "^1.4.2",
    "@vueuse/core": "^8.2.6",
    "@vueuse/motion": "^2.0.0-beta.12",
    "@vueuse/shared": "^8.0.1",
    "ali-oss": "^6.17.1",
    "animate.css": "^4.1.1",
    "axios": "^0.26.0",
    "bignumber.js": "^9.0.2",
    "css-color-function": "^1.3.3",
    "dayjs": "^1.10.8",
    "element-plus": "2.1.11",
    "element-resize-detector": "^1.2.3",
    "html2canvas": "^1.4.1",
    "js-cookie": "^3.0.1",
    "kk-oss": "^0.0.13",
    "kk-ws-client": "^0.0.3",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.2",
    "md5": "^2.3.0",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "pinia": "^2.0.13",
    "qrcode": "^1.5.0",
    "qs": "^6.10.2",
    "resize-observer-polyfill": "^1.5.1",
    "responsive-storage": "^1.0.11",
    "rgb-hex": "^4.0.0",
    "ua-parser-js": "0.7.31",
    "vite-plugin-cdn-import": "^0.3.5",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue": "^3.2.33",
    "vue-cropper": "^1.0.5",
    "vue-demi": "^0.13.11",
    "vue-router": "^4.0.14",
    "vue-types": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@iconify-icons/ep": "^1.2.4",
    "@iconify-icons/fa": "^1.2.2",
    "@iconify-icons/fa-solid": "^1.2.2",
    "@iconify-icons/ri": "^1.2.1",
    "@iconify/vue": "^3.2.0",
    "@types/element-resize-detector": "1.1.3",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/mockjs": "1.0.3",
    "@types/node": "14.14.14",
    "@types/nprogress": "0.2.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-legacy": "^1.8.1",
    "@vitejs/plugin-vue": "^2.3.1",
